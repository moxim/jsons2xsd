{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "$id": {
        "default": "I have not type!"
      },
      "price": {
        "type": "number",
        "minimum": 0
      },
      "name": {
        "type": "string",
        "minLength": 5,
        "maxLength": 32
      },
      "isExpired": {
        "default": false,
        "type": "boolean"
      }
    },
    "required": [
      "price",
      "name"
    ]
  }
}
